@model DataAccess.Models.CommonModel

@{
    int userRole = 0;
    string managerStoreLink = "";
    string AcademyStoreLink = "";
    string HomeUrl = Request.Url.Host;
    string StoreLabel = "";
    if (Model.AcademyId > 0)
    {
        AcademyStoreLink = Model.StoreUrl + "?AcademyId=" + Model.AcademyId.ToString();
        string StoreLoginURL = Model.StoreUrl; // (Session["StoreLoginURL"] == null || Session["StoreLoginURL"] == string.Empty) ? AcademyStoreLink : Session["StoreLoginURL"].ToString();
        AcademyStoreLink = StoreLoginURL;
        HomeUrl = string.IsNullOrEmpty(Model.HomeUrl) ? Request.Url.Host : Model.HomeUrl.ToString();
        HomeUrl = HomeUrl.Contains("http") ? HomeUrl : "http://" + HomeUrl;
        string LessPkg = "58,1149,1186,1187,1197,1201,1202,1205,1206,1208,1209,1214,1215,1224,1225,1226,1227,1228,1217,1218,1219,1220,1221,1222,1223,1194,1196,1189,1190,1139,1191,1188,1193,1118,1157,1159,1182,1186,1187,1199,1145,1178";
        string GlfPkg = "1183";
        string PkgCrdts = "12,13";
        if (LessPkg.Contains(Model.AcademyId.ToString()))
        {
            StoreLabel = "Lesson Packages";
        }
        else if (GlfPkg.Contains(Model.AcademyId.ToString()))
        {
            StoreLabel = "Golf Packages";
        }
        else if (PkgCrdts.Contains(Model.AcademyId.ToString()))
        {
            StoreLabel = "Package Credits";
        }
        else
        {
            StoreLabel = "Store";
        }
        if (Model.UserRoleId > 0)
        {
            userRole = Model.UserRoleId;
            if (userRole == 2)
            {
                managerStoreLink = StoreLoginURL;
            }
        }
    }
}
